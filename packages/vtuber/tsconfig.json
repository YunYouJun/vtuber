{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2019",
    "lib": ["ESNext", "DOM"],
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": ["dist", "node_modules"]
}
